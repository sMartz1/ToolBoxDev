{
  "name": "toolboxdev",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "author": "Sebastian Martinez",
  "homepage": "./",
  "build": {
    "extends": null,
    "appId": "com.toolbox.dev",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "dist/**/*",
      "package.json",
      "mainProcess/**/*"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@craco/craco": "^7.0.0",
    "@nut-tree/nut-js": "^3.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^5.2.1",
    "electron-is-dev": "^2.0.0",
    "node-sass": "^7.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "winston": "^3.8.2"
  },
  "scripts": {
    "setup": "npm run build && electron-builder -c.extraMetadata.main=build/electron.js",
    "buildCraco": "craco build",
    "start": "electron .",
    "str": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "pretest": "electron-rebuild",
    "build":"react-scripts build",
    "postinstall": "electron-builder install-app-deps"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^23.1.0",
    "electron-packager": "^17.1.1",
    "electron-builder": "^23.6.0",
    "electron-rebuild": "^3.2.9",
    "webpack-node-externals": "^3.0.0"
  }
}
